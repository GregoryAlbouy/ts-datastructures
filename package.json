{
  "name": "ts-structures",
  "version": "0.4.1",
  "description": "TypeScript implementation of common data structures",
  "main": "index.js",
  "scripts": {
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "test": "jest",
    "coverage": "jest --coverage",
    "postcoverage": "npm run serve:coverage",
    "codecov": "jest --coverage && codecov -f test/.coverage/coverage-final.json",
    "prepackage:build": "npm test && npm run lint",
    "package:build": "npm run package:build-notest",
    "package:build-notest": "BUILD_PATH=./package BUILD_TSCONFIG=./tsconfig.build.json node ./scripts/build.js",
    "package:clear": "if [[ -d ./package ]]; then rm -r ./package/*; fi",
    "package:publish": "cd ./package && npm publish --access=public",
    "docs": "typedoc --options ./typedoc.config.js",
    "serve:coverage": "SERVE_DOCUMENT=coverage PORT=9998 node ./scripts/serve",
    "serve:docs": "SERVE_DOCUMENT=docs PORT=9997 node ./scripts/serve",
    "clear": "rm -r ./docs ./package ./test/.coverage"
  },
  "keywords": [
    "typescript",
    "data",
    "structures",
    "list",
    "queue",
    "stack",
    "tree",
    "heap",
    "graph"
  ],
  "author": "Gregory Albouy",
  "license": "MIT",
  "homepage": "https://gregoryalbouy-ts-datastructures.netlify.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gregoryalbouy/ts-datastructures.git"
  },
  "bugs": {
    "url": "https://github.com/gregoryalbouy/ts-datastructures/issues"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/reflect-metadata": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "codecov": "^3.7.2",
    "eslint": "^7.9.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-tsdoc": "^0.2.7",
    "express": "^4.17.1",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "typedoc": "^0.19.1",
    "typescript": "^4.0.3"
  }
}
