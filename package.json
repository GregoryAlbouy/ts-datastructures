{
  "name": "typescript-datastructures",
  "version": "1.0.0",
  "description": "TypeScript implementation of common data structures",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "coveralls": "jest --coverage && cat ./test/coverage/lcov.info | coveralls",
    "prebuild": "npm test && npm run lint && npm run dist:clean",
    "build": "tsc",
    "dist:clean": "rm -r ./dist/*",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint .",
    "docs": "typedoc --readme none --out docs src"
  },
  "keywords": [
    "typescript",
    "datasctructures"
  ],
  "author": "Gregory Albouy",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/reflect-metadata": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.9.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "typedoc": "^0.19.1",
    "typescript": "^4.0.3"
  }
}
