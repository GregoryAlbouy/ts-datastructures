{
  "name": "ts-structures",
  "version": "0.2.0",
  "description": "TypeScript implementation of common data structures",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:coveralls": "jest --coverage && cat ./test/.coverage/lcov.info | coveralls",
    "prebuild": "npm test && npm run lint",
    "build": "BUILD_PATH=./package BUILD_TSCONFIG=./tsconfig.build.json node ./scripts/build.js",
    "dist:clean": "if [[ -d ./dist ]]; then rm -r ./dist/*; fi",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "docs": "typedoc --readme none --out docs src",
    "publish": "cd ./package && npm publish --access=public"
  },
  "keywords": [
    "typescript",
    "data",
    "structures",
    "list",
    "queue",
    "stack",
    "tree",
    "heap",
    "graph"
  ],
  "author": "Gregory Albouy",
  "license": "MIT",
  "homepage": "https://gregoryalbouy-ts-datastructures.netlify.app",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gregoryalbouy/ts-datastructures.git"
  },
  "bugs": {
    "url": "https://github.com/gregoryalbouy/ts-datastructures/issues"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/reflect-metadata": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "coveralls": "^3.1.0",
    "eslint": "^7.9.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-tsdoc": "^0.2.7",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "typedoc": "^0.19.1",
    "typescript": "^4.0.3"
  }
}
